{
  "name": "backend-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.js",
    "build": "tsc",
    "start": "node dist/server.js",
    "sequelize": "NODE_ENV=development npx sequelize-cli",
    "db:migrate": "npx sequelize-cli db:migrate --config src/config/config.js",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo --config src/config/config.js",
    "db:seed": "npx sequelize-cli db:seed:all --config src/config/config.js",
    "db:generate:migration": "npx sequelize-cli migration:generate --name ",
    "configs": "--config src/config/config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "inversify": "^6.2.1",
    "inversify-express-utils": "^6.4.10",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.12.0",
    "nodemailer": "^6.10.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.0",
    "@types/sequelize": "^4.28.20",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
